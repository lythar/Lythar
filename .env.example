BACKEND_PATH=C:\Projects\lythar-backend # Scieżka do folderu backendu
FRONTEND_PATH=C:\Projects\lythar-frontend # Scieżka do folderu frontendu

# Path: Lythar/lythar-frontend/.env.production

# Klucz publiczny do weryfikacji tokenów JWT, przykładowy klucz znajduje się w /secrets/public.pem
# Klucz publiczny musi być bez wcięć, w jednej linii tak jak poniżej
JWT_PUBLIC_KEY = '-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----'

# Adres URL backendu
# Przykładowo, jeśli backend działa lokalnie na porcie 8080, to adres URL będzie wyglądał tak: http://localhost:8080
# W przypadku, gdy backend działa na serwerze, to adres URL będzie wyglądał tak: https://example.com
NEXT_PUBLIC_API_URL = 'localhost:8080'

 # Nazwa organizacji
NEXT_PUBLIC_ORGANIZATION_NAME = 'Lythar'